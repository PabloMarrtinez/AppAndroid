<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.OLYMPUS_ExampleMobileApp" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/azul</item>
        <item name="colorPrimaryVariant">#007B80</item>
        <item name="colorOnPrimary">#22468F</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#FEC947</item>
        <item name="colorSecondaryVariant">#CBC13C</item>
        <item name="colorOnSecondary">#8A884C</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="bottonNavigation">
        <item name="android:background">@color/azul</item>
        <item name="itemIconTint">@color/white</item>
        <item name="itemTextColor">@color/white</item>
    </style>
</resources>